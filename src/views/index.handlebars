<div class="contenedorTotal">
{{#each products}}
            <div class="contenedorGeneral card col-sm-1">
            <img class="card-img-top" src="{{this.thumbnail}}" alt="ImagenAleatoria">
            <div class="contenedorBody card-body">
                <h5 class="card-text">{{this.title}}</h5>
                <p class="card-text">{{this.description}} </p>
                <p class="card-text">Precio: {{this.price}} </p>       
                <p class="card-text">Stock: {{this.stock}} </p>   
                <p class="card-text">Code: {{this.code}} </p>                      
            </div>
        </div>
{{/each}}
</div>